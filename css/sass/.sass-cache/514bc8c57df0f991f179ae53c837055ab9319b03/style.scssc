/* =============================================================================
	 2.f &Icons
	 ========================================================================== */
/*  Styling inspired by FontAwesome - http://mopo.ws/ReAOC6
		------------------------------------------------------- */
.icon:before, .icon:before {
	font-family: base-icon-font;
	font-weight: normal;
	font-style: normal;
	display: inline-block;
	text-decoration: inherit;
}
a .icon, a .icon {
	display: inline-block;
	text-decoration: inherit;
}
/* makes the font 33% larger relative to the icon container */
.icon-large:before {
	vertical-align: top;
	font-size: 1.3333333333333333em;
}
.btn .icon, .btn .icon {
	/* keeps button heights with and without icons the same */
	line-height: .5;
}
li .icon, li .icon {
	display: inline-block;
	width: 1.25em;
	text-align: center;
}
li .icon-large[class^="icon-"], li .icon-large[class*=" icon-"] {
	/* 1.5 increased font size for icon-large * 1.25 width */

	width: 1.875em;
}
li.icon, li.icon {
	margin-left: 0;
	list-style-type: none;
}
li.icon:before, li.icon:before {
	text-indent: -2em;
	text-align: center;
}
li.icon.icon-large:before, li.icon.icon-large:before {
	text-indent: -1.3333333333333333em;
}

.icon-anim-rotate {
	-webkit-transform-origin: 49% 38%;
		 -moz-transform-origin: 49% 38%;
			 -o-transform-origin: 49% 38%;
			-ms-transform-origin: 49% 38%;
					transform-origin: 49% 38%;

	-webkit-animation	: rotate 1s infinite linear;
			-moz-animation: rotate 1s infinite linear;
				-o-animation: rotate 1s infinite linear;
			 -ms-animation: rotate 1s infinite linear;
}

/*
 * Use the Unicode Private Use Area (PUA) to ensure screen readers do not
 * read off random characters that represent icons
 */

/* WEB APPLICATION ICONS U+e0xxx --------------------------------------------- */
.icon-checkbox-unchecked:before	{ content: "\e000";}
.icon-checkbox-checked:before		{ content: "\e001";}

.icon-radio-unchecked:before		{ content: "\e010";}
.icon-radio-checked:before			{ content: "\e011";}

.icon-loader:before 						{ content: "\e020";}

.icon-reorder:before						{ content: "\e030";}

.icon-remove:before							{ content: "\e040";}

.icon-comment:before						{ content: "\e050";}

.icon-calendar:before						{ content: "\e060";}


/* TEXT EDITOR ICONS U+e1xxx ------------------------------------------------- */
.icon-font:before 							{ content: "\e100";}
.icon-bold:before 							{ content: "\e110";}
.icon-italic:before 						{ content: "\e120";}
.icon-underline:before 					{ content: "\e130";}
.icon-strikethrough:before			{ content: "\e140";}

.icon-link:before 							{ content: "\e150";}
.icon-unlink:before 						{ content: "\e151";}

.icon-list-ol:before						{ content: "\e160";}
.icon-list-ul:before						{ content: "\e161";}

.icon-image:before							{ content: "\e170";}

.icon-undo:before								{ content: "\e180";}
.icon-redo:before								{ content: "\e181";}


/* DIRECTIONAL ICONS U+e2xxx ------------------------------------------------- */
.icon-caret-up:before						{ content: "\e200";}
.icon-caret-down:before					{ content: "\e201";}
.icon-caret-right:before				{ content: "\e202";}
.icon-caret-left:before					{ content: "\e203";}

/* VIDEO / AUDIO ICONS U+e3xxx ----------------------------------------------- */


/* SOCIAL ICONS U+e4xxx ------------------------------------------------------- */
.icon-facebook:before { content: "\e400";}
.icon-twitter:before { content: "\e410";}


/* =============================================================================
