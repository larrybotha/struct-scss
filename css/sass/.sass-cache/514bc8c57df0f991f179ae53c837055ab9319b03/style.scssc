/* =============================================================================
	 2.e &Forms
	 ========================================================================== */
.field, .field-label { position: relative;}

input, textarea {
	margin-bottom: 1em;
	padding: 0 0 0 .1em;
	border: 1px solid #ccc;
}

/*
 * Required only if using icon prefix and suffix blocks.
 * Addresses FF's use of !important on line-heights
 * on inputs making normalising difficult - http://mopo.ws/SvxClq
 *
 * fix radios and inputs affected by new input height
 */
input	{	height: 1.75em;}
input[type="radio"],
input[type="checkbox"] { height: auto;}

.prefix-icon-container,
.suffix-icon-container {
	position: relative;
	display: inline-block;
}

.prefix-icon-container > input { padding-left: 1.5em;}
.suffix-icon-container > input { padding-right: 1.5em;}

.prefix-icon-container > .icon,
.suffix-icon-container > .icon,
.prefix-icon-block,
.suffix-icon-block {
	vertical-align: top;
	display: inline-block;
	width: 1.75em;
	height: 1.75em;
	text-align: center;
	padding-top: .05em;
}

.prefix-icon-container > .icon,
.suffix-icon-container > .icon {
	position: absolute;
	margin-top: .1em;
	padding-top: 0;
}

.suffix-icon-container > .icon {
	position: relative;
	margin-left: -1.75em;
}

.prefix-icon-block, .suffix-icon-block {
	background: #f2f2f2;
	border: 1px solid #ccc;
	margin-top: -1px;
}

.radio-custom:before,
.checkbox-custom:before { margin-bottom: .5em;}

.radio-custom:before { content: "\e010";}
.checkbox-custom:before { content: "\e000"; /* .icon-check-empty - FontAwesome */}

input:checked + label > .radio-custom:before { content: "\e011";}
input:checked + label > .checkbox-custom:before { content: "\e001"; /* .icon-check - FontAwesome */}

/* -----------------------------------------------------------------------------
	 2.e.i &Date Picker
	 -------------------------------------------------------------------------- */
.ui-datepicker {
	display: none;
	background-color: #222;
	font-size: 12px;
	font-size: .75rem;
	padding: 1em;
	-webkit-border-radius: 2px;
		 -moz-border-radius: 2px;
					border-radius: 2px;
}

.ui-datepicker,
.ui-datepicker a { color: #bbb;}

.ui-datepicker .ui-datepicker-header {
	position: relative;
	padding: .3em 0 .4em;
	background-color: #333;

	-webkit-border-radius	: 2px;
			-moz-border-radius: 2px;
						border-radius: 2px;

	-webkit-box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 0%, .05);
					box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 0%, .05);
}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next {
	position:absolute;
	text-indent: -100em;
	top: 2px;
	width: 1.8em;
	height: 1.8em;
	cursor: pointer;
}

.ui-datepicker .ui-datepicker-prev:before,
.ui-datepicker .ui-datepicker-next:before {
	font-family: base-icon-font;
	position: absolute;
	text-indent: 0;
	text-align: center;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	padding-top: .25em;
}

.ui-datepicker .ui-datepicker-prev { left:2px;}
.ui-datepicker .ui-datepicker-prev:before { content: "\e203";}
.ui-datepicker .ui-datepicker-next { right:2px;}
.ui-datepicker .ui-datepicker-next:before { content: "\e202";}
.ui-datepicker .ui-datepicker-prev-hover { left:1px;}
.ui-datepicker .ui-datepicker-next-hover { right:1px;}

.ui-datepicker .ui-datepicker-title { text-align: center;}

.ui-datepicker table {
	font-size: .9em;
	width: 100%;
	border-collapse: collapse;
	margin:0 0 .4em;
}
.ui-datepicker th {
	font-weight: bold;
	text-align: center;
	padding: .7em .3em;
	border: 0;
}
.ui-datepicker td {
	padding: 0 1px 3px;
	border: 0;
}
.ui-datepicker td span, .ui-datepicker td a {
	display: block;
	padding: .4em .5em .3em;
	text-align: right;
	text-decoration: none;
	border: 1px solid #333;
}

.ui-datepicker td a:hover { border-color: #444;}

.ui-datepicker td .ui-state-highlight { background: #333;}

/* RTL support */
.ui-datepicker-rtl { direction: rtl;}
.ui-datepicker-rtl .ui-datepicker-prev {
	right: 2px;
	left: auto;
}
.ui-datepicker-rtl .ui-datepicker-next {
	left: 2px;
	right: auto;
}
.ui-datepicker-rtl .ui-datepicker-prev:hover {
	right: 1px;
	left: auto;
}
.ui-datepicker-rtl .ui-datepicker-next:hover {
	left: 1px;
	right: auto;
}
.ui-datepicker-rtl .ui-datepicker-buttonpane { clear:right;}
.ui-datepicker-rtl .ui-datepicker-buttonpane button { float: left;}
.ui-datepicker-rtl .ui-datepicker-buttonpane button.ui-datepicker-current { float:right;}
.ui-datepicker-rtl .ui-datepicker-group { float:right;}
.ui-datepicker-rtl .ui-datepicker-group-last .ui-datepicker-header {
	border-right-width:0;
	border-left-width:1px;
}
.ui-datepicker-rtl .ui-datepicker-group-middle .ui-datepicker-header {
	border-right-width:0;
	border-left-width:1px;
}

/* -----------------------------------------------------------------------------
	 2.e.ii &Autocomplete
	 -------------------------------------------------------------------------- */
.ui-autocomplete {
	position: absolute;
	cursor: pointer;
	background: $clr-ntrl-min;
	font-size: .75em;
	border: 1px solid #b2b2b2;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
}

.ui-menu {
	list-style:none;
	padding: 0;
	margin: 0;
}
.ui-menu .ui-menu {
	margin-top: -3px;
}
.ui-menu .ui-menu-item { border-bottom: 1px solid #eee;}
.ui-menu .ui-menu-item a {
	display: block;
	color: inherit;
	padding: .4em .5em;
	line-height: 1.5;
}
.ui-menu .ui-menu-item a.ui-state-hover,
.ui-menu .ui-menu-item a.ui-state-active {
	font-weight: normal;
	background-color: #e4eef5;
	background-image: -webkit-linear-gradient(top, transparent, hsla(0, 0%, 100%, .1));
	background-image:    -moz-linear-gradient(top, transparent, hsla(0, 0%, 100%, .1));
	background-image:      -o-linear-gradient(top, transparent, hsla(0, 0%, 100%, .1));
	background-image:     -ms-linear-gradient(top, transparent, hsla(0, 0%, 100%, .1));
	background-image:         linear-gradient(top, transparent, hsla(0, 0%, 100%, .1));
}

.ui-menu-item .img-block > .img {
	background: transparent url(http://placekitten.com/g/20/20) no-repeat center center;
	width: 20px;
	height: 20px;
	margin-right: .5em;
}

.token-list {
	position: relative;
	display: block;
	border: 1px solid #ccc;
	cursor: text;
	padding-left: .25em;
}

.token-list + .ui-autocomplete { left: 0 !important;}

.token-list-input {
	background-color: transparent;
	padding: 0;
	margin: 0;
	border: 0;
	outline: none;
}

.token-list-item {
	display: inline-block;
	vertical-align: middle;
}

.token-list-token {
	vertical-align: middle;
	font-size: 75%;
	line-height: 1;
	padding: .25em .5em;
	border: 1px solid #aaa;
	-webkit-border-radius: 2px;
		 -moz-border-radius: 2px;
					border-radius: 2px;
}

