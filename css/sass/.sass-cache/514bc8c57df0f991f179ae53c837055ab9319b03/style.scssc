/* rows hold columns - should be used with .cf */
.grid-example .col {
	padding: ($unit-default-px * .5) $unit-default-px;
	padding: .5rem 1rem;
	background: #eee;
	border-radius: 2px;
	font-family: monospace;
	text-align: center;
}

.row { margin-left: -$unit-default-px;}

.ie8 .row,
.ie7 .row { margin-left: 0;}

/* Reusable column setup */
.col {
	border: 0 solid hsla(0,0%,0%,0);
	border-left-width: $unit-default-px;
	border-left-width: 1rem;
	float: left;
	margin-bottom: $unit-default-px;
	margin-bottom: 1rem;
	padding: 0;
	position:  relative;
	/* already declared at base styles - must be uncommented if not declared */
	/*-webkit-box-sizing: border-box;
		 -moz-box-sizing: border-box;
					box-sizing: border-box;*/
	-webkit-background-clip: padding-box !important;
		 -moz-background-clip: padding-box !important;
					background-clip: padding-box !important;
}

/*
 * Restrict inline-block cols to modern browsers.
 * Inline-block columns are useful when floated columns of different heights
 * prevent subsequent columns from floating to their appropriate positions
 */
:root .row-ib-cols { margin: 0 (-$unit-default-px * .6) 0 (-$unit-default-px * .6);}
:root .row-ib-cols > .col {
	float: none;
	display: inline-block;
	vertical-align: top;
	margin-left: (-$unit-default-px * .6);
