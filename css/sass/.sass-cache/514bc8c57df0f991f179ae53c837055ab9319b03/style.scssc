/*
 * media object - http://mopo.ws/GJYch3
 * .mblock and .mblock-beta should always be used in conjunction with
 * .cf
 *
 * Improvements by Nicholas Gallagher - http://jsfiddle.net/necolas/rZvEF/
 *
 * .mblock {}
 * .mblock > .mblock-alpha {}
 */
.mblock:before, .mblock:after { content: ""; display: table; height:0;}
.mblock:after { clear: both;}

.mblock > .mblock-beta {
	display: table-cell; /* 1 */
	width: 10000px; /* 2 */
}
.mblock .mblock-alpha { float: left; margin-right: 12px; margin-right: 1rem;}
.mblock .mblock-alpha img { display: block;}


/* =============================================================================
	 2.c &Navigation
	 ========================================================================== */
/*
 * Nav abstraction - use classes specific to particular nav
 * to differentiate - http://mopo.ws/zadlrO
 */
.menu {
	list-style: none;
	margin-left: 0;
	padding-left: 0;
}

.menu > .menu-item {
	position: relative;
	display: inline;
}

.menu-item > a { display: inline-block;}
.ie7 .menu-item > a { zoom: 1;}

.sub-menu {
	position: absolute;
	display: none;
	z-index: 100;
}

.menu-collapse-container .menu-collapse-button {
	background-image:     -ms-linear-gradient(top, hsla(0, 0%, 0%, .2), hsla(0, 0%, 0%, .2));
	background-image: -webkit-linear-gradient(top, hsla(0, 0%, 0%, .2), hsla(0, 0%, 0%, .2));
	background-image:      -o-linear-gradient(top, hsla(0, 0%, 0%, .2), hsla(0, 0%, 0%, .2));
	background-image:         linear-gradient(top, hsla(0, 0%, 0%, .2), hsla(0, 0%, 0%, .2));
	background-image:    -moz-linear-gradient(top, hsla(0, 0%, 0%, .2), hsla(0, 0%, 0%, .2));

	-webkit-box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 0%, .05);
					box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 0%, .05);

	margin: .5em;
}

.menu-collapse-container .menu-collapse-button,
.menu-collapse-container .menu-collapse-button:hover,
.menu-collapse-container .menu-collapse-button:active {
	background-color: transparent;
	color: $clr-ntrl-min;
}

.menu-collapse-container { background-color: #222;}
.menu-collapse { display: none;}
.menu-collapse-checkbox:checked + .menu-collapse { display: block;}

.menu-primary,
.menu {	background-color: #222;}

.menu-primary .menu-item a {
	display: block;
	color: #ddd;
	padding: .5em 1em;
	border-top: 1px solid hsla(0, 0%, 100%, .02);
	border-bottom: 1px solid hsla(0, 0%, 0%, .2);
}

.menu-primary .menu-item a:hover { background-color: $clr-default;}

.menu-primary .sub-menu { position: relative;}

.menu-primary .sub-menu a {	padding-left: 2em;}

.menu-primary .sub-menu .sub-menu a {	padding-left: 4em;}

.menu-item a:hover + .sub-menu,
.sub-menu:hover {	display: block;}

@media all and (min-width: 800px) {
	.menu-collapse-container { display: none;}
	.menu-collapse { display: block;}

	.menu-primary .menu-item,
	.menu-primary .menu-item a { display: inline-block;}

	.menu-primary .sub-menu {
		position: absolute;
		border-right: 1px solid hsla(0, 0%, 0%, .2);
		 border-left: 1px solid hsla(0, 0%, 0%, .2);
	}

	.menu-primary .sub-menu .sub-menu { top: 0; left: 100%;}

	.menu-primary .sub-menu a,
	.menu-primary .sub-menu .sub-menu a {
		min-width: 225px;
		padding-left: 1em;
		white-space: nowrap;
	}
}

/* =============================================================================
	 2.d &Buttons
	 ========================================================================== */
.btn {
	position: relative;
	display: inline-block;
	background-color: #f5f5f5;
	background-image:     -ms-linear-gradient(top, hsla(0, 0%, 100%, .2), hsla(0, 0%, 90%, .2));
	background-image: -webkit-linear-gradient(top, hsla(0, 0%, 100%, .2), hsla(0, 0%, 90%, .2));
	background-image:      -o-linear-gradient(top, hsla(0, 0%, 100%, .2), hsla(0, 0%, 90%, .2));
	background-image:         linear-gradient(top, hsla(0, 0%, 100%, .2), hsla(0, 0%, 90%, .2));
	background-image:    -moz-linear-gradient(top, hsla(0, 0%, 100%, .2), hsla(0, 0%, 90%, .2));
	font-size: 15px;
	font-size: 1.071rem;
	font-weight: normal;
	line-height: 1;
	color: $clr-ntrl-max;
	margin-bottom: 1em;
	padding: .3em 1em;
	vertical-align: middle;
	text-align: center;
	cursor: pointer;

	border: 1px solid #cccccc;
	border-color: #e6e6e6 #e6e6e6 #bfbfbf;
	border-color: hsla(0, 0%, 0%, .1) hsla(0, 0%, 0%, .1) rgba(0, 0, 0, 0.25);

	-webkit-border-radius: 2px;
		 -moz-border-radius: 2px;
					border-radius: 2px;

	/* android 4.0 requires prefix */
	-webkit-box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 90%, .05);
					box-shadow: inset 0 1px 0 hsla(0, 0%, 100%, .2), 0 1px 2px hsla(0, 0%, 90%, .05);

	-webkit-transition: all 150ms ease-in;
		 -moz-transition: all 150ms ease-in;
			 -o-transition: all 150ms ease-in;
			-ms-transition: all 150ms ease-in;
					transition: all 150ms ease-in;
}

.btn:hover {
	background-color: #ededed;
	color: $clr-ntrl-max;
}

.btn:active, .btn-selected, .btn-selected:hover {
	background-color: #ededed;
	color: $clr-ntrl-max;
	/* android 4.0 requires prefix */
	-webkit-box-shadow: inset 0 1px 3px hsla(0, 0%, 0%, .5), 0 1px 2px hsla(0, 0%, 90%, .05);
					box-shadow: inset 0 1px 3px hsla(0, 0%, 0%, .5), 0 1px 2px hsla(0, 0%, 90%, .05);

	-webkit-transition: all 0;
		 -moz-transition: all 0;
			 -o-transition: all 0;
			-ms-transition: all 0;
					transition: all 0;
}

.btn-selected { cursor: default;}

.btn[disabled], .btn.disabled {
	color: #999;
	cursor: default;
}
.btn[disabled], .btn.disabled,
.btn:hover[disabled], .btn.disabled:hover,
.btn:active[disabled], .btn.disabled:active {
	background-color: $clr-ntrl-min;
	box-shadow: none;
}

.btn-group { display: inline-block;}

.btn-group > .btn {
	float: left;
	margin-right: 0;
	margin-left: -1px;
	-webkit-border-radius: 0;
		 -moz-border-radius: 0;
					border-radius: 0;
}

.btn-group > .btn:first-child,
.btn-group > .btn:last-child,
.btn-group > .btn:first-child:last-child {
	-webkit-border-radius: 2px;
		 -moz-border-radius: 2px;
					border-radius: 2px;
}

.btn-group > .btn:first-child {
	margin-left: 0;
	border-top-right-radius: 0;
	border-bottom-right-radius: 0;
}

.btn-group > .btn:last-child {
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}

