					focus:-moz-placeholder { color: #CCC;}
						focus:-o-placeholder { color: #CCC;}
					 focus:-ms-placeholder { color: #CCC;}


/* =============================================================================
	 &Tables
	 ========================================================================== */

table { border-collapse: collapse; border-spacing: 0;}
